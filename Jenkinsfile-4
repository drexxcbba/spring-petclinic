#!groovy

pipeline {
  agent any
  tools {
     maven 'maven-3'
  }
  stages {
    stage('Generate jar') {
      steps {
        sh './mvnw clean package'
      }
    }
    stage('Upload to Artifactory'){
      steps{
        sh 'curl -X PUT admin:JjVv135@2k22 -T /target/spring-petclinic-2.6.0-SNAPSHOT.jar http://192.168.0.23:8081/artifactory/libs-snapshot-local/spring-petclinic-2.6.0-SNAPSHOT.jar'
      }
    }
  }
} 
